#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Sources/visual-schema-builder/node_modules/.pnpm/vite@6.4.1_@types+node@22.1_b987aaa0b8f167f228de3b72b9cc3f2f/node_modules/vite/bin/node_modules:/mnt/e/Sources/visual-schema-builder/node_modules/.pnpm/vite@6.4.1_@types+node@22.1_b987aaa0b8f167f228de3b72b9cc3f2f/node_modules/vite/node_modules:/mnt/e/Sources/visual-schema-builder/node_modules/.pnpm/vite@6.4.1_@types+node@22.1_b987aaa0b8f167f228de3b72b9cc3f2f/node_modules:/mnt/e/Sources/visual-schema-builder/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Sources/visual-schema-builder/node_modules/.pnpm/vite@6.4.1_@types+node@22.1_b987aaa0b8f167f228de3b72b9cc3f2f/node_modules/vite/bin/node_modules:/mnt/e/Sources/visual-schema-builder/node_modules/.pnpm/vite@6.4.1_@types+node@22.1_b987aaa0b8f167f228de3b72b9cc3f2f/node_modules/vite/node_modules:/mnt/e/Sources/visual-schema-builder/node_modules/.pnpm/vite@6.4.1_@types+node@22.1_b987aaa0b8f167f228de3b72b9cc3f2f/node_modules:/mnt/e/Sources/visual-schema-builder/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
